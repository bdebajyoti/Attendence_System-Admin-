<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="com.selfowner.att_student">
<uses-permission android:name="android.permission.INTERNET"/>
    <application
        android:allowBackup="true"
        android:icon="@mipmap/stuapplogo"
        android:label="@string/app_name"
        android:roundIcon="@mipmap/stuapplogo"
        android:supportsRtl="true"
        android:theme="@style/AppTheme">
        <activity android:name=".View_Attendance"></activity>
        <activity android:name=".Keypad_Attendence" />
        <activity android:name=".Personal_Info" />
        <activity android:name=".Dashboard" />
        <activity android:name=".Stu_Login" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>

        <meta-data
            android:name="preloaded_fonts"
            android:resource="@array/preloaded_fonts" />
    </application>

</manifest>